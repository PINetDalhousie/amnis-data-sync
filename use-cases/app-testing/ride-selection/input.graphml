<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
     http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

  <!-- graph attributes -->
  <key id="topicCfg" for="graph" attr.name="topicConfig" attr.type="string"/>
  
  <!-- node attributes -->
  <key id="zk" for="node" attr.name="zookeeper" attr.type="int"/>
  <key id="bk" for="node" attr.name="broker" attr.type="int"/>
  <key id="prodType" for="node" attr.name="producerType" attr.type="string"/>   <!-- producer type can be SFST/MFMT/RND; MFMT,RND not supported right now. -->
  <key id="prodCfg" for="node" attr.name="producerConfig" attr.type="string"/>  <!-- for SFST: one pair of filePath, topicName; for MFMT: multiple pair of filePaths, topicNames; -->
  <key id="consCfg" for="node" attr.name="consumerConfig" attr.type="string"/>  <!-- consConfig will contain the topic name(s) from where the consumer will consume --> 
  <key id="sparkCfg" for="node" attr.name="sparkConfig" attr.type="string"/>    <!-- sparkConfig will contain the input source,
                                                                    spark application path and output sink. Input source is a
                                                                    kafka topic, output sink can be kafka topic/a file directory. --> 

  <!-- edge attributes -->
  <key id="bw" for="edge" attr.name="bandwidth" attr.type="int"/>
  <key id="lat" for="edge" attr.name="latency" attr.type="int"/>
  <key id="st" for="edge" attr.name="sport" attr.type="int"/>
  <key id="dt" for="edge" attr.name="dport" attr.type="int"/>
  
   
  <graph id="simple-topo" edgedefault="undirected">
    <data key="topicCfg">/home/ubuntu/Desktop/amnis-data-sync/use-cases/app-testing/ride-selection/topicConfiguration.txt</data>

    <node id="h1">
      <data key="zk">1</data>
      <data key="bk">1</data>
      <data key="prodType">SFST</data>
      <data key="prodCfg">/home/ubuntu/Desktop/amnis-data-sync/use-cases/app-testing/ride-selection/nycTaxiRidesData.csv,taxirides,1</data>
      <data key="consCfg">taxirides,1</data>
	  </node>            
    
    <node id="s1"/>
    <node id="s2"/>

    <node id="h2">
      <data key="bk">1</data>
      <data key="prodType">SFST</data>
      <data key="prodCfg">/home/ubuntu/Desktop/amnis-data-sync/use-cases/app-testing/ride-selection/nycTaxiFaresData.csv,taxifares,1</data>
      <data key="consCfg">taxifares,1</data>
      <data key="sparkCfg">/home/ubuntu/Desktop/amnis-data-sync/use-cases/app-testing/ride-selection/rideSelection.py,/home/ubuntu/Desktop/amnis-data-sync/logs</data>
	  </node>            

    <edge source="h1" target="s1">
		<data key="bw">1</data>
		<data key="lat">20</data>
		<data key="st">1</data>
		<data key="dt">1</data>
    </edge>
    <edge source="s1" target="s2">
		<data key="bw">5</data>
		<data key="lat">10</data>
		<data key="st">3</data>
		<data key="dt">2</data>
    </edge>
    <edge source="s2" target="h2">
		<data key="bw">1</data>
		<data key="lat">20</data>
		<data key="st">1</data>
		<data key="dt">1</data>
    </edge>
  </graph>
</graphml>
